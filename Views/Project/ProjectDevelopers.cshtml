
@model List<Developer>;
<br />
<div class="container">
    <div class="row">

        <div class="col-sm-8"> <h2><img src="~/assets/img/LOGO.png" width="30%" alt="Alternate Text" /> <b>PROJECT DEVELOPERS</b> </h2></div>


        <table class="table table-responsive-sm" style="text-align:center">
            <thead class="thead-dark">
                <tr>

                    <th>First Name</th>
                    <th>Last Name</th>



                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    var Developer = item;
                    <tr>
                        <td>@Developer.FirstName</td>
                        <td>@Developer.LastName</td>

                    </tr>
                }
            </tbody>
        </table>

        <br /><br /><br />
        <div class="row">

            <div class="col-sm-12 mt-4"> <a href="/Project/ShowProjects" type="button" class="btn LeftColer" style=" margin-top: -10px; margin-bottom: 26px;"> Go Back</a> </div>

        </div>

  
    </div>

    <style>
        th, td {
            text-align: center;
        }
    </style>
